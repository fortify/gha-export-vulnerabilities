{
  "name": "gha-export-vulnerabilities",
  "version": "1.0.0",
  "description": "GitHub Action for exporting Fortify vulnerability data to various targets",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write **/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add dist/"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortify/gha-export-vulnerabilities.git"
  },
  "keywords": [
    "Fortify",
    "FoD",
    "security"
  ],
  "author": "Ruud Senden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fortify/gha-export-vulnerabilities/issues"
  },
  "homepage": "https://github.com/fortify/gha-export-vulnerabilities#README.md",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/tool-cache": "^1.6.1",
    "@actions/exec": "^1.0.4",
    "@actions/io": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "@vercel/ncc": "0.27.0",
    "eslint": "^7.21.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}

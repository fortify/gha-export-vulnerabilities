{
  "name": "gha-export-vulnerabilities",
  "version": "1.0.1",
  "description": "GitHub Action for exporting Fortify vulnerability data to various targets",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortify/gha-export-vulnerabilities.git"
  },
  "keywords": [
    "Fortify",
    "FoD",
    "security"
  ],
  "author": "Ruud Senden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fortify/gha-export-vulnerabilities/issues"
  },
  "homepage": "https://github.com/fortify/gha-export-vulnerabilities#README.md",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^1.7.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.56",
    "@vercel/ncc": "0.31.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "prettier": "^2.7.1",
    "typescript": "4.4.3"
  }
}
